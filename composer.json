{
    "name": "simpod/php-snmp",
    "description": "PHP Simple Network Management",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Podlipsky",
            "email": "simon@podlipsky.net"
        }
    ],
    "keywords": [
        "php",
        "snmp"
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.2",
        "bentools/iterable-functions": "^1.2",
        "guzzlehttp/guzzle": "^6.3"
    },
    "suggest": {
        "ext-json": "Required for SNMP transport \"Api\"",
        "ext-snmp": "Required for SNMP transport \"Extension\""
    },
    "require-dev": {
        "ext-curl": "*",
        "ext-json": "*",
        "ext-snmp": "*",
        "doctrine/coding-standard": "^6.0",
        "jakub-onderka/php-parallel-lint": "^1.0",
        "phpstan/phpstan": "^0.11.5",
        "phpstan/phpstan-strict-rules": "^0.11.0",
        "phpunit/phpunit": "^8.2"
    },
    "autoload": {
        "psr-4": {
            "SimPod\\PhpSnmp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SimPod\\PhpSnmp\\Tests\\": "tests/"
        }
    }
}
